<style>
.container-full {
  margin: 0 auto;
  width: 100%;
}
</style>


<!DOCTYPE html>
<html>
  <head>
    <title>CustomerWeb</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <%= render 'layouts/header' %>

    <div class="container-full">
      <%= yield %>
      <%= render 'layouts/footer' %>
    </div>
  </body>
</html>
